        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Aim inputs

var axis_x = 0,
    axis_y = 0;
if settings_get_control_mode_wasd() {
    axis_x = keyboard_check(vk_right) - keyboard_check(vk_left);
    axis_y = keyboard_check(vk_down) - keyboard_check(vk_up);
} else {
    axis_x = keyboard_check(ord("D")) - keyboard_check(ord("A"));
    axis_y = keyboard_check(ord("S")) - keyboard_check(ord("W"));
}

if (axis_x != 0 || axis_y != 0) {
    // The angle that we're aiming at rigidly
    var target_angle = point_direction(0, 0, axis_x, axis_y);
    var adiff = angle_difference(target_angle, _aim_angle);
    var spd = clamp(10 * adiff / 45, 8, 30) * get_dtime();
    if abs(adiff) &lt; spd {    
        _aim_angle = target_angle;
    } else {
        _aim_angle += sign(adiff) * spd;
    }
}
</string>
