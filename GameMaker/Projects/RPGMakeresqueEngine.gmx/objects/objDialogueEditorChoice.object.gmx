var editButtonBuffer = 32;

var padding = 4;

var h = padding;

_editTextButton.x = x + padding;
_editTextButton.y = y + padding;
_deleteButton.x = x + _width - padding - 32;
_deleteButton.y = y + padding;


// The max length of our text
var caplength = _width - padding*2 - editButtonBuffer - string_width("...") - 32;

// The sub text we draw
var sub = _text;

if string_width(_text) &gt; caplength {
    sub = string_copy(_text, 0, (caplength / string_width("a")) - 3);
    sub += "...";
}

draw_set_color(c_black);
draw_text(x + padding + editButtonBuffer, y + h, '"' + sub + '"');

h += 32 + padding;

for(var i = 0; i &lt; ds_list_size(_choices); i++) {
    var choiceElement = _choices[| i];
    choiceElement.x = x;
    choiceElement.y = y + h;
    
    h += padding + choiceElement._height;
}

_plusButton.x = x + _width/2 - 16;
_plusButton.y = y + h;

h += 32 + padding;

_height = h;


d3d_transform_set_identity();
</string>
