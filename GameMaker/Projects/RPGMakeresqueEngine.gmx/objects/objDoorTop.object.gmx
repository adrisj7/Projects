            <string>/// Is the player moving into us?
var player = other;

if !_locked {
    // Is the player moving towards us?
    if player._facing == _direction {
        var entering = false;
        switch _direction {
            case DIRECTION.UP:
                entering = player._moveInputY == -1;
                break;
            case DIRECTION.DOWN:
                entering = player._moveInputY == 1;
                break;
            case DIRECTION.LEFT:
                entering = player._moveInputX == -1;
                break;
            case DIRECTION.RIGHT:
                entering = player._moveInputX == 1;
                break;
        }
        // Now actually transition!
        if entering {
            game_set_state(GAME_STATE.DIALOGUE);
            image_speed = 0.2;
            overlay_set_darkness(1);
        }
    }
}
</string>
