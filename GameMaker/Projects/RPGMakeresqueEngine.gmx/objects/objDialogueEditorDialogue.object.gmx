            <kind>1</kind>
            <string>/// Draw stuff

editor_apply_view_transformation();

draw_set_font(font_debug);

editor_draw_window(x, y, _width, _height, c_black, $a9c6a5);

var padding = 8;
var h = padding;

// Draw name
draw_set_halign(fa_center);
draw_set_color(c_black);
draw_text(x+_width/2, y+h, _name);
draw_set_halign(fa_left);

h += string_height(_name);

var maxWidth = _width;

// Draw elements
for(var i = 0; i &lt; ds_list_size(_elements); i++) {
    var element = _elements[| i];
    element.x = x + padding;
    element.y = y + h;
    h += element._height + padding;

    if (element._width + 2*padding &gt; maxWidth) {
        maxWidth = element._width + 2*padding;
    }
}

_plusElementButton.x = x + _width/2 - 16;
_plusElementButton.y = y + h;

_deleteDialogueButton.x = x + _width - padding - 32;
_deleteDialogueButton.y = y + h;
_renameDialogueButton.x = x + _width - padding*2 - 32*2;
_renameDialogueButton.y = y + h;

h += 32;
h += padding;

_width = maxWidth;
_height = h;

d3d_transform_set_identity();
</string>
