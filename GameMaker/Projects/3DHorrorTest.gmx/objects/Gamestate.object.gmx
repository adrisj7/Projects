var length = 32;
var dx = lengthdir_x(length, view_yaw);
var dy = lengthdir_y(length, view_yaw);
var dz = lengthdir_y(length, view_pitch);

// Up vector (2 matrix transformations, do the math)
// (first apply roll (x axis), then apply yaw (z axis) )
var up_x =      lengthdir_y(1, view_roll) * lengthdir_y(1, view_yaw),
    up_y = -1 * lengthdir_y(1, view_roll) * lengthdir_x(1, view_yaw),
    up_z = lengthdir_x(1, view_roll);
    
d3d_set_projection(view_x, view_y, view_z, view_x + dx, view_y + dy, view_z + dz, up_x, up_y, up_z);


