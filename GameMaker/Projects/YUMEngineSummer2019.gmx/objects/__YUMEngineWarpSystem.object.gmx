        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Warping
if _warping {
    // Fully faded: Switch rooms
    if screen_get_fade_percent() == 1 {
        // First frame: Go to the next room
        if room != _target_room {
            room_goto(_target_room);
        } else {
            printf("Warping Player!");
            // Second frame: We're in the room, so set our target!
            screen_set_fade(0, _warp_duration/2);

            // Check for warp object
            var obj = _target_object_index;
            if obj != noone {
                if !object_exists(obj) {
                    show_error("Invalid warp: object index is invalid: " + string(obj), true);
                }
                if !instance_exists(obj) {
                    show_error('Invalid warp: Object of instance type "' + object_get_name(obj) + '" present in room!', false);
                    _target_tile_x = 0;
                    _target_tile_y = 0;
                } else {
                    _target_tile_x = tilemap_global_get_tile_x(obj.x);
                    _target_tile_y = tilemap_global_get_tile_y(obj.y);
                }
            }

            // Now warp the player
            var player = character_get_player();
            character_set_position(player, _target_tile_x, _target_tile_y);
            character_set_orientation(player, _target_orientation);
        }
    } else if screen_get_fade_percent() == 0 {
        _warping = false;
    }
}
</string>
