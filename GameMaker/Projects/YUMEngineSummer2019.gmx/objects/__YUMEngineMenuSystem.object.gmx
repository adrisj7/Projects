        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Fading and Menu opening


if _fading {
    if _fade_closing {
        if screen_get_fade_percent() == 1 {
            // JUST FADED OUT
            //show_debug_message("[DEBUG] Faded DARK");
            screen_set_fade(0, _fade_duration);
            _fade_closing = false;
            // Disable our previous menu, enable our new menu
            var new = __menu_get_next_type();
            var newr = __menu_get_system_renderer(new);
            if newr != noone {
                newr._active = true;
                //show_debug_message("[DEBUG]    NEW: " + string(new));
            }// else show_debug_message("[DEBUG]    NEW: (none)");
            //__menu_stack_pop();
            var old = __menu_get_current_type();
            var oldr = __menu_get_system_renderer(old);
            if oldr != noone {
                oldr._active = false;
                //show_debug_message("[DEBUG]    OLD: " + string(old));
            }// else show_debug_message("[DEBUG]    OLD: (none)");
            // Set our current menu
            _menu_current = new;
        }
    } else {
        if screen_get_fade_percent() == 0 {
            // JUST UNFADED BACK
            //show_debug_message("[DEBUG] Faded LIGHT");
            //show_debug_message("[DEBUG]    CURRENT: " + string(__menu_get_current_type()));
            // Resume player control
            _fading = false;
            // If we got no more menus, resume control
            if __menu_get_current_type() == MenuType.None {
                __menu_force_close();
            }
        }
    }
}

//show_debug_message(menu_is_open());
</string>
