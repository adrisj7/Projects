        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Handle progress and events

if _opening {
    if _open_duration == 0 {
        _progress = 1;
    } else {
        var delta = 1.0 / _open_duration;
        // Opening or open
        if _progress &lt; 1 {
            _progress += delta;
            if _progress &gt;= 1 {
                _progress = 1;
            }
        }
    }
} else {
    if _close_duration == 0 {
        _progress = 0;
    } else {
        var delta = 1.0 / _close_duration;

        if _progress &gt; 0 {
            _progress -= delta;
            if _progress &lt;= 0 {
                _progress = 0;
            }
        }
    }
}

</string>
