        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Check for Selection and cancel

if _active &amp;&amp; dialogue_opener_is_open(_opener) {
    var chosen = false;
    if __dialogue_check_progress_pressed() {
        chosen = true;
    }
    if __dialogue_check_cancel_pressed() &amp;&amp; _default_choice &gt;= 0 {
        chosen = true;
        _choice_selected = _default_choice;
    }

    // If we picked something, run it and close!
    if chosen {
        var script = _scripts[@ _choice_selected];
        if script_exists(script) {
            __dialogue_queue_stack_push();
//            show_debug_message("[DEBUG] EXECUTING SCRIPT: " + script_get_name(script));
            script_execute(script);
        }
        dialogue_opener_close(_opener);

        // Close text, if we got one open.
        __dialogue_stop_text();
    }
}

</string>
