        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Change choice by Input

if _active &amp;&amp; dialogue_opener_is_open(_opener) {
    var delta =  __dialogue_check_down() - __dialogue_check_up();

    var size = array_length_1d(_choices);

    // Just to make sure we don't get weird negative results
    if size == 0 delta = 0;

    _choice_selected += delta;

    // Wrap
    if _choice_selected &lt; 0 {
        _choice_selected += size;
    } else if _choice_selected &gt;= size {
        _choice_selected -= size;
    }
}
</string>
