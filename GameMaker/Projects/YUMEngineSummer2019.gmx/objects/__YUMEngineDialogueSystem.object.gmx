            <string>/// Auto Menu: Inputs and menu
if _auto_menu {
    dialogue_progress(keyboard_check(ord("Z")));
    dialogue_cancel(keyboard_check(ord("X")));
    dialogue_up(keyboard_check_pressed(vk_up));
    dialogue_down(keyboard_check_pressed(vk_down));

